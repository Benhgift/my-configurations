    statusBar: "Show all errors"
  "linter-js-yaml": {}
  "linter-jshint": {}
  "markdown-preview":
    useGitHubStyle: true
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "trailing-spaces":
    enableForIndentation: true
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    removeTrailingWhitespace: true
