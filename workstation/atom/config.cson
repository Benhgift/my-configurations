"*":
  "":
    slim:
      text:
        editor:
          preferredLineLength: 130
          showIndentGuide: true
          showInvisibles: false
    source:
      yml:
        "tabs-to-spaces":
          onSave: "none"
  "autocomplete-js-import":
    fuzzy:
      enabled: true
  "autocomplete-ruby":
    rsensePath: "/Users/chrishough/.rbenv/shims/rsense"
  core:
    destroyEmptyPanes: false
    disabledPackages: [
      "tabs-to-spaces"
      "language-cfml"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".svn"
      ".DS_Store"
      ".bundle"
    ]
    telemetryConsent: "no"
    themes: [
      "atom-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  editor:
    fontFamily: "Inconsolata"
    fontSize: 15
    invisibles: {}
    softWrap: true
    tabType: "soft"
  "exception-reporting":
    userId: "d9f8a28e-3694-e767-4e3e-180303f43226"
  "glowing-cursor":
    cursorWidth: 2
    glowColor: "#ff2739"
